{
  "name": "consent-control",
  "version": "0.1.0",
  "description": "Consent external sources, cookies, analytics,... with user-centered design",
  "main": "dist/index.js",
  "style": "css/consentcontrol.css",
  "files": [
    "dist/",
    "src/",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build:js": "rollup -c",
    "build:css": "node build-css.js",
    "watch:css": "npm-watch",
    "version": "npm run build:js && npm run build:css && git add .",
    "postversion": "git push && git push --tags && npm publish --access=public"
  },
  "watch": {
    "build:css": {
      "patterns": [
        "src"
      ],
      "extensions": "scss",
      "quiet": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mmoollllee/consent-message.git"
  },
  "keywords": [
    "cookie",
    "consent",
    "privacy",
    "gdpr"
  ],
  "author": "Moritz Graf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmoollllee/consent-message/issues"
  },
  "homepage": "https://github.com/mmoollllee/consent-message#readme",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-babel": "^5.3.1",
    "core-js": "^3.22.5",
    "npm-watch": "^0.11.0",
    "prettier": "2.6.2",
    "pretty-quick": "^3.1.3",
    "rollup": "^2.72.1",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "1.51.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {}
}
